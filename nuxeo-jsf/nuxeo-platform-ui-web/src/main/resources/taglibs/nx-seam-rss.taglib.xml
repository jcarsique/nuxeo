<?xml version="1.0" encoding="UTF-8"?>

<facelet-taglib id="nxrss"
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
  version="2.2">
  <description>Nuxeo Seam RSS Taglib Override</description>
  <namespace>http://nuxeo.org/nxweb/seam/rss</namespace>
  <tag>
    <tag-name>feed</tag-name>
    <component>
      <component-type>
        org.nuxeo.ecm.platform.ui.web.component.seam.UIFeed
      </component-type>
    </component>
  </tag>

  <tag>
    <tag-name>entry</tag-name>
    <component>
      <component-type>
        org.nuxeo.ecm.platform.ui.web.component.seam.UIEntry
      </component-type>
    </component>
  </tag>
</facelet-taglib>
