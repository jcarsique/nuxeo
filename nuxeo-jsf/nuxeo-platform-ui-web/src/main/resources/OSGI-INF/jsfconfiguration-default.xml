<component name="org.nuxeo.ecm.platform.ui.web.runtime.JSFConfigurationService.default">

  <extension target="org.nuxeo.ecm.platform.ui.web.runtime.JSFConfigurationService"
             point="configuration">

    <configuration>
      <property name="nuxeo.jsf.enableDoubleClickShield">${nuxeo.jsf.enableDoubleClickShield:=true}</property>
      <property name="nuxeo.jsf.useAjaxTabs">${nuxeo.jsf.useAjaxTabs:=false}</property>
      <property name="nuxeo.jsf.listWidget.compatEnabled">${nuxeo.jsf.listWidget.compatEnabled:=false}</property>
      <property name="nuxeo.jsf.removeAliasOptims">${nuxeo.jsf.removeAliasOptims:=false}</property>
    </configuration>

  </extension>

</component>
