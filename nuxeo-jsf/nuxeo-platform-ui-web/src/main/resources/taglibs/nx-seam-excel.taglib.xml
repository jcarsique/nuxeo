  version="2.2">
  <description>Nuxeo Seam Excel Taglib Override</description>
  <namespace>http://nuxeo.org/nxweb/seam/excel</namespace>
  <tag>
    <tag-name>column</tag-name>
    <component>
      <component-type>
        org.nuxeo.ecm.platform.ui.web.component.seam.UIColumn
